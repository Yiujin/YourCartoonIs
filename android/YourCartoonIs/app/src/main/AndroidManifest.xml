        <activity android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>

                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".Pictransfer" >
        </activity>
        <activity android:name=".ProgressActivity" >
        </activity>
        <activity android:name=".VideoTransfer">
        </activity>
        <activity android:name=".Dummy">
        </activity>
        <activity android:name=".ShowPictransferResult">

        </activity>

    <activity android:name=".MakeCollage">
    </activity>
        <activity android:name=".StickerActivity" >
        </activity>
        <activity android:name=".AfterRecvActivity" >
        </activity>
        <activity
